function Car(brand, model, year) {
    this.brand = brand,
   this.model = model,
    this.year = year,
    
    this.showDetails = function(){
        console.log(`Brand: ${this.brand}, Model: ${this.model}, Year: ${this.year}`);
    }
};

let a = new Car("Toyota","Fortuner",2023);

a.showDetails();

